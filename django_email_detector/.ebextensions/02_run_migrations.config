container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && /var/app/staging/manage.py migrate;"
    leader_only: true
  02_makemigrate:
    command: "source /var/app/venv/*/bin/activate && /var/app/staging/manage.py makemigrations app_email_detector;"
    leader_only: true
